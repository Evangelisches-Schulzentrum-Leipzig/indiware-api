{
  "dependencies": {
    "ajv": "^6.10.2",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.138",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.7.4",
    "@types/request-promise-native": "^1.0.16",
    "tslint": "^5.19.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.2",
    "typescript-json-schema": "^0.40.0"
  },
  "scripts": {
    "build": "npm run lint && npm run build:ts && npm run build:schema",
    "build:ts": "tsc",
    "build:schema": "typescript-json-schema --noExtraProps --required --strictNullChecks -o ./build/xml/xmljsonschema.json ./src/xml/xmlschema.ts XmlFileSchema",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "start": "node build"
  }
}
