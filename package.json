{
  "dependencies": {
    "ajv": "^6.10.2",
    "basic-auth": "^2.0.1",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "node-fetch": "^3.2.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.138",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "typescript": "^4.4.3",
    "typescript-json-schema": "^0.53.0"
  },
  "scripts": {
    "build": "npm run lint && npm run build:ts && npm run build:schema",
    "build:ts": "tsc",
    "build:schema": "typescript-json-schema --noExtraProps --required --strictNullChecks -o ./build/xml/xmljsonschema.json ./src/xml/xmlschema.ts XmlFileSchema",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "node build"
  },
  "type": "module"
}
